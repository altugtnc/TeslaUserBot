@register(outgoing=True, pattern="^Selam$")

async def oof(e):
    t = "Selam"
    for j in range(16):
        t = t[:-1] + "am"
        await e.edit(t)

PENIS_TEMPLATE = """
â™¥â•â•â•â•â™«â•â•â•â•â•â•â™¥
â–‘â–‘â–„â–€â–€â–„â–„â–„â–€â–€â–„â–‘â–‘
â–‘â–ˆâ–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–ˆâ–‘
â–‘â–€â–„â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–„â–€â–‘
â–‘â–‘â–‘â–€â–„â–‘â–‘â–‘â–„â–€â–‘â–‘â–‘
â–‘â–‘â–‘â–‘â–‘â–€â–„â–€â–‘â–‘â–‘â–‘â–‘
â™¥â•â•â•â•â™«â•â•â•â•â•â•â™¥.
"""
@register(outgoing=True, pattern="^.sevgi (.*)")
async def emoji_sevgi(e):
    emoji = e.pattern_match.group(1)

    await e.edit("Seni Seviyorum...")
    message = PENIS_TEMPLATE
    if emoji:
        message = message.replace('ğŸ†', emoji)

    await e.edit(message)

@register(outgoing=True, pattern="^Siktir$")

async def oof(e):
    t = "siktir"
    for j in range(16):
        t = t[:-1] + "ir"
        await e.edit(t)

@register(outgoing=True, pattern="^.tata (.*)")
async def tata(event):
    if event.fwd_from:
        return
    input_str = event.pattern_match.group(1)
    await event.edit("ï¸_/ï¹‹\_\n(Ò‚`_Â´)\n<,ï¸»â•¦â•¤â”€ Ò‰ - - " + input_str +"\n_/ï¹‹\_")


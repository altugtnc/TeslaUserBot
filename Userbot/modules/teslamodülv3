@register(outgoing=True, pattern="^.ask (.*)")
async def ask (event):
    if event.fwd_from:
        return
    input_str = event.pattern_match.group(1)
    await event.edit("ï¸â•”â•â•â•—â•”â•— â™¡ â™¡ â™¡\nâ•šâ•—â•”â•â•‘â•‘â•”â•â•¦â•¦â•¦â•”â•—\nâ•”â•â•šâ•—â•‘â•šâ•£â•‘â•‘â•‘â•‘â•”â•£\nâ•šâ•â•â•â•šâ•â•©â•â•©â•â•©â•â•\nâ™¡" + input_str +"â™¡\nğ“¢ğ“”ğ“ğ“˜ ğ“¢ğ“”ğ“¥ğ“˜ğ“¨ğ“ğ“¡ğ“¤ğ“œ")

@register(outgoing=True, pattern="^.bye")

async def bye(event):

    if event.fwd_from:

        return

    animation_interval = 0.3

    animation_ttl = range(0, 11)

    await event.edit("Ben Gidiyorum â¤ï¸")

    animation_chars = [
            "â¤ï¸=ğŸ™‹â€â™‚ï¸====â¤ï¸",
            "â¤ï¸==ğŸ™‹â€â™‚ï¸===â¤ï¸",
            "â¤ï¸===ğŸ™‹â€â™‚ï¸==â¤ï¸",
            "â¤ï¸====ğŸ™‹â€â™‚ï¸=â¤ï¸",
            "â¤ï¸=====ğŸ™‹â€â™‚ï¸â¤ï¸",
            "â¤ï¸====ğŸ™‹â€â™‚ï¸=â¤ï¸",
            "â¤ï¸===ğŸ™‹â€â™‚ï¸==â¤ï¸",
            "â¤ï¸==ğŸ™‹â€â™‚ï¸===â¤ï¸",
            "â¤ï¸=ğŸ™‹â€â™‚ï¸====â¤ï¸",
            "â¤ï¸ğŸ™‹â€â™‚ï¸=====â¤ï¸",
            "Bye Bye â¤ï¸",
			]

    for i in animation_ttl:

        await asyncio.sleep(animation_interval)

        await event.edit(animation_chars[i % 11])

@register(outgoing=True, pattern="^bruh$")

async def oof(e):
    t = "bruh"
    for j in range(16):
        t = t[:-1] + "uh"
        await e.edit(t)

PENIS_TEMPLATE = """
â•”â•¦â•¦
â• â•¬â•¬â•¬â•£
â• â•¬â•¬â•¬â•£ I â™¥
â• â•¬â•¬â•¬â•£ Chocolate
â•šâ•©â•©â•©â• CeteUserBot
"""

@register(outgoing=True, pattern=r"^\.(?:cho)\s?(.)?")
async def emoji_nah(e):
    emoji = e.pattern_match.group(1)

    await e.edit("Cete...")
    message = PENIS_TEMPLATE
    if emoji:
        message = message.replace('ğŸ†', emoji)

    await e.edit(message)


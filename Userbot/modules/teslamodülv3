@register(outgoing=True, pattern=r"^\.(?:yrk|dick)\s?(.)?")
async def emoji_yrk(e):
    emoji = e.pattern_match.group(1)

    await e.edit("Al BakalÄ±m...")
    message = PENIS_TEMPLATE
    if emoji:
        message = message.replace('ğŸ˜', emoji)

    await e.edit(message)


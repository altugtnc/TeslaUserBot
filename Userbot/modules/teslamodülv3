@register(outgoing=True, pattern="^.plane")
async def port_plane(event):
    if event.fwd_from:
        return


    await event.edit("âœˆ-------------")
    await asyncio.sleep(1)
    await event.edit("-âœˆ------------")
    await asyncio.sleep(1)
    await event.edit("--âœˆ-----------")
    await asyncio.sleep(1)
    await event.edit("---âœˆ----------")
    await asyncio.sleep(1)
    await event.edit("----âœˆ---------")
    await asyncio.sleep(1)
    await event.edit("-----âœˆ--------")
    await asyncio.sleep(1)
    await event.edit("------âœˆ-------")
    await asyncio.sleep(1)
    await event.edit("-------âœˆ------")
    await asyncio.sleep(1)
    await event.edit("--------âœˆ-----")
    await asyncio.sleep(1)
    await event.edit("---------âœˆ----")
    await asyncio.sleep(1)
    await event.edit("----------âœˆ---")
    await asyncio.sleep(1)
    await event.edit("-----------âœˆ--")
    await asyncio.sleep(1)
    await event.edit("------------âœˆ-")
    await asyncio.sleep(1)
    await event.edit("-------------âœˆ")
    await asyncio.sleep(5)
    await event.delete()

@register(outgoing=True, pattern="^.police")

async def port_police(event):

    if event.fwd_from:

        return

